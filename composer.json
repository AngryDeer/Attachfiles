{
    "name": "angrydeer/attachfiles",
    "type": "library",
    "description": "Add some attached files to Eloquent model and store it to storage/app/attached/{Y-m-d} dir. ",
    "keywords": [
        "AngryDeer",
        "Attachfiles"
    ],
    "homepage": "https://github.com/AngryDeer/Attachfiles",
    "license": "MIT",
    "authors": [
        {
            "name": "AngryDeer",
            "email": "dimkin.pivovarov@gmail.com",
            "homepage": "http://angrydeer.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Angrydeer\\Attachfiles\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Angrydeer\\Attachfiles\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
